<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel = "tests" name="Suite">
<listeners><listener class-name="akhiraacademy.TestComponents.Listeners"></listener></listeners>
  <test thread-count="5" name="SubmitTest">
    <classes>
      <class name="akhiraacademy.Tests.FinalSolution"/>
      
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="ErrorTest">
    <classes>
      <class name="akhiraacademy.Tests.ErrorValidations"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
